<div class="row">
    {{#each products}}
    <div class="col-sm-6 col-md-4">
        <div class="thumbnail">
            <img src="" alt="..." class="img-responsive">
            <div class="caption">
                <h3>{{this.Title}}</h3>
                <p class="desc">{{this.Description}}</p>
                <div class="clearfix">
                    <div class="price">{{this.Price}}</div>
                    <a href="/add-to-cart/{{this._id}}" class="btn btn-success pull-right" role="button">Add to Cart</a></p>

                </div>
                
            </div>
        </div>
    </div>
    {{/each}}
</div>

